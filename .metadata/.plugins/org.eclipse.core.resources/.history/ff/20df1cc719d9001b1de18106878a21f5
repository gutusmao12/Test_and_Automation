package POO;

import java.util.Scanner;

public class vini {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [] primeiroNum = new int [primeiroNum.length];
		int [] segundoNum = new int [1];
		int contador = 0;
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Digite o primeiro número: ");
		primeiroNum[1] = sc.nextInt();
		System.out.println("Digite o segundo número: ");
		segundoNum[1] = sc.nextInt();
		
		for(int i = 0; i < primeiroNum.length; i++) {
			for(int j = 0; j < segundoNum.length; j++) {
				if(primeiroNum[i] != segundoNum[j]) {
					contador++;
				}
			}
		}
		return;
	}

}
